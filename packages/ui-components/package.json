{
  "name": "@snap-studio/ui-components",
  "version": "1.0.0",
  "description": "Ant Design based UI components library for snap-studio low-code platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
